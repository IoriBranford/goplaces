{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Server",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "server",
        "cwd": "${workspaceFolder}/server"
      },
      {
        "name": "Client",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/client/src",
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${webRoot}/*"
        }
      }
    ]
  }